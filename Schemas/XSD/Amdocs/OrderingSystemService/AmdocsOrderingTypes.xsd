    <xs:complexType name="CreditInfoType">
        <xs:annotation>
            <xs:documentation>The credit status of a customer entity. This is a pure ML response entity that is not part of the customer persistance but maintains the response output of the credit check (internal/external) connectors.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="creditClass" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Credit class of the client.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="blackListIndicator" type="FlagType" use="optional">
            <xs:annotation>
                <xs:documentation>Indicates if the customer is in a black list. Used to identify potential fraud cases.
</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="blackListReason" type="BlackListReasonType" use="optional">
            <xs:annotation>
                <xs:documentation>Reason that a customer is in a black list.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="creditStatus" type="CreditStatusType" use="optional">
            <xs:annotation>
                <xs:documentation>Credit status of customer - returned by externalCreditCheck interface.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="SiteType">
        <xs:annotation>
            <xs:documentation>Holds the siteId value when returend from the GetSiteId contract</xs:documentation>
        </xs:annotation>
        <xs:attribute name="siteID" type="SiteIDType">
            <xs:annotation>
                <xs:documentation> The site id value </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="PayerType">
        <xs:annotation>
            <xs:documentation>A reference to a paying entity in an external billing system, for example: a pay channel or a billing account.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" ref="SubscriptionGroup"/>
        </xs:sequence>
        <xs:attribute name="externalID" type="IDType" use="required">
            <xs:annotation>
                <xs:documentation>An identifier of the paying entity in the external billing system, for example: Pay Channel ID or BAN</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="previousExternalID" type="IDType" use="optional">
            <xs:annotation>
                <xs:documentation>The previous paying entity if different from the current.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="invalidIndicator" type="FlagType" use="optional">
            <xs:annotation>
                <xs:documentation>Indicates if the payer is valid or not.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="description" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Description of the payer.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="payingPurpose" type="PayingPurposeType" use="optional">
            <xs:annotation>
                <xs:documentation>The purpose for which the payer is defined).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="parentId" type="IDType" use="optional">
            <xs:annotation>
                <xs:documentation>ID of the parent element.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="paymentCategory" type="xs:string" use="optional"/>
        <xs:attribute name="name" type="NameType" use="optional"/>
    </xs:complexType>
    <xs:complexType name="ConnectionUserType">
        <xs:annotation>
            <xs:documentation>The connection user for the installation address.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="title" type="TitleType" use="optional">
            <xs:annotation>
                <xs:documentation>.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="firstName" type="NameType" use="optional">
            <xs:annotation>
                <xs:documentation>For a private individual, the private or first name </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="lastName" type="NameType" use="optional">
            <xs:annotation>
                <xs:documentation>For a private individual, the family name or surname (e.g. Smith)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="SubscriptionGroupType">
