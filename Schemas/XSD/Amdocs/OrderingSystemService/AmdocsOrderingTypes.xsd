<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://mtel.bg/Amdocs/Ordering/AmdocsOrderingTypes" version="1.0" xmlns="http://mtel.bg/Amdocs/Ordering/AmdocsOrderingTypes" xmlns:AmdocsOrderingElements="http://mtel.bg/Amdocs/Ordering/AmdocsOrderingElements" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:annotation>
        <xs:documentation>IntBusObjTypes.xsd - Base 1300, ExtBusObjTypes.xsd - Base 1300, Domains.xsd Base - 1300, DynamicProducts.xsd - Base 1300, IntBusObjElements.xsd - Base 1300, ExtBusObjElements.xsd - Base 1300, Context.xsd - Base 1300, OmsContext.xsd - Base 1300</xs:documentation>
    </xs:annotation>
    <!-- Include the domain file -->
    <xs:complexType name="RuleMessageParamType">
        <xs:annotation>
            <xs:documentation>A parameter for the message</xs:documentation>
        </xs:annotation>
        <xs:attribute name="value" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>A value of a parameter for a message.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="OrderType">
        <xs:annotation>
            <xs:documentation>An order in OMS is the top level ordering unit for a customer. An order is a container of one or serveral order actions.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:annotation>
                <xs:documentation> Contacts - list of contact persons for the order. Remarks - list of remarks attached to the order. Project - the project that the order is associated to.</xs:documentation>
            </xs:annotation>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="OrderContact"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="Remark"/>
            <xs:element minOccurs="0" ref="Project"/>
        </xs:sequence>
        <xs:attribute name="orderID" type="IDType" use="required">
            <xs:annotation>
                <xs:documentation>A unique identifier of the Order in OMS. This ID is generated by OMS.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="referenceNumber" type="IDType">
            <xs:annotation>
                <xs:documentation>The order's internal reference number.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="externalReferenceNumber" type="IDType">
            <xs:annotation>
                <xs:documentation>A unique identifier of the order in an external system. For example, the corresponding Quote ID in CRM.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="customerReferenceNumber" type="IDType">
            <xs:annotation>
                <xs:documentation>An identifier of the order, as received from the customer.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="opportunityID" type="IDType">
            <xs:annotation>
                <xs:documentation>The corresponding opportunity ID in CRM.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="applicationDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The date upon which the order was submitted.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderMode" type="OrderModeType">
            <xs:annotation>
                <xs:documentation>The mode in which the order is negotiated (e.g. Proposal, Delivery).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderStatus" type="OrderStatusType">
            <xs:annotation>
                <xs:documentation>The status of the order (e.g. Initial, Executing, Done).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderStatusName" type="NameType">
            <xs:annotation>
                <xs:documentation>The decode of the status of the order (e.g. Initial, Executing, Done).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="totalPrice" type="xs:decimal">
            <xs:annotation>
                <xs:documentation>Calculated from the total charges on each order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="dueDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>Calculated from the latest due date of all the order actions.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="serviceRequiredDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The requested end date for delivery.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="proposalName" type="IDType"/>
        <xs:attribute name="proposalExpiryDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The expiration date for the order if it is negotiated in proposal mode.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="customerID" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the customer who is the owner of the order.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="operatorPortingIndicator" type="FlagType">
            <xs:annotation>
                <xs:documentation>Indicates whether the order includes Number Porting order actions.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="NPRId" type="IDType">
            <xs:annotation>
                <xs:documentation>The Number Portability Request ID (if operatorPortingIndicator is Yes).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="asapFlag" type="FlagType">
            <xs:annotation>
                <xs:documentation>Indicates that the order should be completed as soon as possible.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="salesChannel" type="SalesChannelType">
            <xs:annotation>
                <xs:documentation>The sales channel for the order.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="creatorID" type="IDType">
            <xs:annotation>
                <xs:documentation>The user that create the order.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="currency" type="CurrencyType">
            <xs:annotation>
                <xs:documentation>The currency that the orderâ€™s prices is based on.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="contactID" type="xs:string">
            <xs:annotation>
                <xs:documentation>The caller contact ID.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rootCustomerID" type="IDType" use="optional"/>
        <xs:attribute name="creationDate" type="xs:dateTime" use="optional">
            <xs:annotation>
                <xs:documentation>The order creation date.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="invoiceNumber" type="StringType">
            <xs:annotation>
                <xs:documentation>The invoice number as returned from billing systems in the Create Invoice interface. Can be multiple number concatenated.
							</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="invoiceMode" type="InvoiceModeType">
            <xs:annotation>
                <xs:documentation>The invoice mode value as result of payment type selected in Order Summary.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="isInvoiceClosed" type="xs:boolean">
            <xs:annotation>
                <xs:documentation>Indicates if the invoice payment in Billing system is closed.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="invoiceDueDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>Due date for the invoice request to be received in AR.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="OrderActionType">
        <xs:annotation>
            <xs:documentation>An order action records and implements a single customer request. Each order action has a type indicating the action required, e.g. Provide, Change, Cease etc.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:annotation>
                <xs:documentation> Contacts - list of contact persons for the order action. Remarks - list of remarks attached to the order action. Attachments - list of attachments attached to the order action.</xs:documentation>
            </xs:annotation>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="OrderContact"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="OAAdditionalStatus"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="Remark"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="Attachment"/>
        </xs:sequence>
        <xs:attribute name="ID" type="IDType" use="required">
            <xs:annotation>
                <xs:documentation>A unique identifier for the order action. This ID is generated by OMS.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderMode" type="OrderModeType" use="optional">
            <xs:annotation>
                <xs:documentation>A unique identifier for the order action. This ID is generated by OMS.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="referenceNumber" type="IDType">
            <xs:annotation>
                <xs:documentation>The order actions's internal reference number.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderActionType" type="OrderActionNameType" use="required">
            <xs:annotation>
                <xs:documentation>The type of order action (e.g. Provide, Change, Cease etc.).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderActionTypeName" type="DescriptionType" use="optional">
            <xs:annotation>
                <xs:documentation>The Decode of the OrderActionType.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ownerID" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the team that is assigned to the order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="receptionDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The date upon which the order action was registered.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="serviceRequiredDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The requested end date for delivery.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="dueDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The expected end date for delivery.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="earlyDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The earliest date for starting the delivery.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderActionStatus" type="OrderActionStatusType">
            <xs:annotation>
                <xs:documentation>The status of the order action (e.g. In Negotiation, In Delivery).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="priority" type="PriorityType">
            <xs:annotation>
                <xs:documentation>The priority given to the order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="appointmentDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The first date set for appointment.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderID" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the OMS order containing the order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderRef" type="IDType">
            <xs:annotation>
                <xs:documentation>The reference number of the OMS order containing the order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderActionGroupID" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the order action group containing this order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="projectNumber" type="IDType">
            <xs:annotation>
                <xs:documentation>The number of the OMS project containing the order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="amendIndicator" type="AmendIndicatorType">
            <xs:annotation>
                <xs:documentation>The type of amend operation if this request is related to another request. Simple - for redo or amend of an order action. Cancel - for cancelling an order action. AddGroup - for adding an order action to a group already in provisioning. Replace - for replacing a previous order action (e.g. for migrating an ordered product).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="referenceOrderAction" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the related order action, if the amendIndicator is not empty.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="reason" type="IDType">
            <xs:annotation>
                <xs:documentation>A reason code indicating why the order action was initiated. This depends also on the context of the order action (e.g. the reason for amending an order action, the reason for a retroactive order action etc.).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="reasonText" type="DescriptionType">
            <xs:annotation>
                <xs:documentation>A free text description for the reason if the reason is Other</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="asapFlag" type="FlagType">
            <xs:annotation>
                <xs:documentation>Indicates that the order action should be completed as soon as possible.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="infrastructureConflict" type="InfrastructureConflictType">
            <xs:annotation>
                <xs:documentation>Reason for Manual TSI when negotiating an installation address. For example, separate intro cable to be installed, connection point to be moved etc.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="InfrastructureConflictFreeText" type="DescriptionType">
            <xs:annotation>
                <xs:documentation>Free text description of the reason for a manual TSI when the infrastructureConflict is not empty.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="customerID" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the customer who is the owner of the order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="apID" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the product associated with this order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="productStatus" type="APStatusType">
            <xs:annotation>
                <xs:documentation>The status of the product associated with this order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="productSubStatus" type="APSubStatusType">
            <xs:annotation>
                <xs:documentation>The sub status of the product associated with this order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="salesChannel" type="SalesChannelType">
            <xs:annotation>
                <xs:documentation>The sales channel for the order action.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="externalReferenceNumber" type="IDType">
            <xs:annotation>
                <xs:documentation>A unique identifier of the order action in an external system.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="externalReferenceRemark" type="DescriptionType">
            <xs:annotation>
                <xs:documentation>A free text remark associated with the external reference number.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="customerReferenceNumber" type="IDType">
            <xs:annotation>
                <xs:documentation>An identifier of the order action, as received from the customer.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="quantity" type="xs:int" use="optional">
            <xs:annotation>
                <xs:documentation>For filtering quantity order action in tracking service</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="dealerCode" type="IDType" use="optional">
            <xs:annotation>
                <xs:documentation>The dealer Code of the parent order.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="shopID" type="xs:string">
            <xs:annotation>
                <xs:documentation>Identifier to the shop when the CSR invoked the Order Action from.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="prepaidBalance" type="xs:decimal">
            <xs:annotation>
                <xs:documentation>The prepaid balance to be migrate to postpaid, or the initial balance when migrating to prepaid.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="prepaidICCID" type="xs:string">
            <xs:annotation>
                <xs:documentation>The prepaid SIM card serial number.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="invoiceNumber" type="xs:string">
            <xs:annotation>
                <xs:documentation>The invoice number as returned from billing systems in the Create Invoice interface. Can be multiple number concatenated.
							</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="invoiceMode" type="InvoiceModeType">
            <xs:annotation>
                <xs:documentation>The invoice mode value as result of payment type selected in Order Summary.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="isInvoiceClosed" type="xs:boolean">
            <xs:annotation>
                <xs:documentation>Indicates if the invoice payment in Billing system is closed.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="invoiceDueDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>Due date for the invoice request to be received in AR.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="campaignID" type="xs:string">
            <xs:annotation>
                <xs:documentation>Holds the campaign ID if exists.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="printMethod" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Holds the print method sent to CSM system.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="printShopInd" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation>Holds the shop indicator sent to CSM system.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="RemarkType">
        <xs:annotation>
            <xs:documentation>A remark consists of free text attached to an order action, assigned product etc.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="remarkType" type="RemarkTypeType" use="required">
            <xs:annotation>
                <xs:documentation>The type of remark (e.g. Technical, Sales).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="remarkText" type="DescriptionType" use="required">
            <xs:annotation>
                <xs:documentation>The remark's text.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="updatedBy" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the user who created or last updated the remark.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="lastModificationDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The date the remark was created or last updated.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="parentId" type="IDType" use="optional">
            <xs:annotation>
                <xs:documentation>ID of the parent element.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractProductItemType">
        <xs:annotation>
            <xs:documentation>Base type for all product items.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:annotation>
                <xs:documentation>Product Catalogue definition. </xs:documentation>
            </xs:annotation>
            <xs:element minOccurs="0" ref="CatalogItem"/>
            <xs:element minOccurs="0" ref="RelationDefinition"/>
            <xs:element minOccurs="0" ref="ItemDetails"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="PricePlan"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="ChargeInfo"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="Payer"/>
            <xs:choice minOccurs="0">
                <xs:element ref="Location"/>
                <xs:element ref="Address"/>
            </xs:choice>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="Deposit"/>
        </xs:sequence>
        <xs:attribute name="APID" type="IDType" use="required">
            <xs:annotation>
                <xs:documentation>A unique identifier of the product in OMS. This is an ID generated by OMS.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="versionID" type="IDType">
            <xs:annotation>
                <xs:documentation>A unique identifier of the product's version in OMS. Each order action creates a new version.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="serviceID" type="ServiceIDType">
            <xs:annotation>
                <xs:documentation>A unique identifier of the service within the Telco. This ID is usually generated by the Telco.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="primaryIdentifier" type="ServiceIDType">
            <xs:annotation>
                <xs:documentation>A primary identifier of the service within the Telco. This ID is usually generated by the Telco.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="siteID" type="SiteIDType">
            <xs:annotation>
                <xs:documentation>Site ID of the selected address site.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="action" type="ItemActionType">
            <xs:annotation>
                <xs:documentation>The requested action on the item for ICS requests (Add, Remove or Update)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="relationID" type="IDType">
            <xs:annotation>
                <xs:documentation>The Product Catalog relation ID this item implements</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="caption" type="NameType">
            <xs:annotation>
                <xs:documentation>The Product Catalog Caption name</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="bulkIndicator" type="FlagType" use="optional">
            <xs:annotation>
                <xs:documentation>Indicates if the item can be sold in bulk quantities.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="bulkQuantity" type="QuantityType" use="optional">
            <xs:annotation>
                <xs:documentation>Bulk quantity of items.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractProductType">
        <xs:annotation>
            <xs:documentation>Base type for all products.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="AbstractProductItemType">
                <xs:sequence>
                    <xs:annotation>
                        <xs:documentation>Remarks - list of remarks attached to the product. Attachments - list of attachments attached to the product.</xs:documentation>
                    </xs:annotation>
                    <xs:element maxOccurs="unbounded" minOccurs="0" ref="Remark"/>
                    <xs:element maxOccurs="unbounded" minOccurs="0" ref="Attachment"/>
                    <xs:element minOccurs="0" ref="ProductDefinition"/>
                    <xs:element minOccurs="0" ref="OfferDefinition"/>
                </xs:sequence>
                <xs:attribute name="customerID" type="IDType">
                    <xs:annotation>
                        <xs:documentation>The ID of the customer who is the owner of the product.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="orderActionID" type="IDType">
                    <xs:annotation>
                        <xs:documentation>The ID of the order action for the product (filled only if the product is in ordered state).</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="timeZoneID" type="StringType" use="optional">
                    <xs:annotation>
                        <xs:documentation>The Java time zone ID.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ProductRefType">
        <xs:annotation>
            <xs:documentation>A reference to another product is used to identify an associated product within other products.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="APID" type="IDType" use="required">
            <xs:annotation>
                <xs:documentation>A unique identifier of the assigned product in OMS.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="versionID" type="IDType">
            <xs:annotation>
                <xs:documentation>A unique identifier of the product's version in OMS. Each order action creates a new version.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="serviceID" type="ServiceIDType">
            <xs:annotation>
                <xs:documentation>A unique identifier of the service within the Telco. This ID is usually generated by the Telco.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="orderActionID" type="IDType">
            <xs:annotation>
                <xs:documentation>The ID of the order action for the referenced product (filled only if the referenced product is in ordered state).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="action" type="ItemActionType">
            <xs:annotation>
                <xs:documentation>The requested action on the reference for ICS requests (Add, Remove or Update)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="dominantApId" type="IDType" use="optional">
            <xs:annotation>
                <xs:documentation>ID of the dominant product.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="externalID" type="IDType" use="optional">
            <xs:annotation>
                <xs:documentation>ID received from external system.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
