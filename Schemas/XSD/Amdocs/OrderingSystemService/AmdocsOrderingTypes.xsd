    <xs:complexType name="ContactType">
        <xs:annotation>
            <xs:documentation>A contact is a person the Telco can refer to when further information is required for business objects such as subscriber order action etc.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="ID" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>A unique identifier of the contact within the Telco's system. This may be either an internal or external ID. It is used by OMS as a foreign key for the contact(s) in an Order, an Order Action etc.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="firstName" type="NameType">
            <xs:annotation>
                <xs:documentation>The private or first name (e.g. John).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="lastName" type="NameType">
            <xs:annotation>
                <xs:documentation>The family name or surname (e.g. Smith).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="title" type="TitleType">
            <xs:annotation>
                <xs:documentation>The form of address (Mr., Mrs., Dr., etc.).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="phoneNumber" type="PhoneNumberType">
            <xs:annotation>
                <xs:documentation>The contact's main phone number.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="mobilePhone" type="PhoneNumberType">
            <xs:annotation>
                <xs:documentation>The contact's mobile phone number.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="email" type="EmailType">
            <xs:annotation>
                <xs:documentation>The contact's e-mail address.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="language" type="LanguageType">
            <xs:annotation>
                <xs:documentation>The default language to use with the contact.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="contactType" type="ContactTypeType">
            <xs:annotation>
                <xs:documentation>The type of entity the contact represents, e.g. the Customer, the Telco, a Third Party etc.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="contactCategory" type="ContactCategoryType">
            <xs:annotation>
                <xs:documentation>The area of responsibility, e.g., Administrative, Technical, Sales etc.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="preferredContactMethod" type="ContactMethodType">
            <xs:annotation>
                <xs:documentation>The preferred method of contact, e.g. Phone, Mobile, Email</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="fromDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The date from which the contact can be approached.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="toDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>The date until which the contact can be approached.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="fromTime" type="xs:time">
            <xs:annotation>
                <xs:documentation>The time of day starting from which the contact can be approached.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="toTime" type="xs:time">
            <xs:annotation>
                <xs:documentation>The time of day until which the contact can be approached.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
