            <xs:documentation>Represents the entity in CRM hierarchy that originates the Order.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="id" type="IDType" use="required"/>
        <xs:attribute name="name" type="NameType" use="optional"/>
